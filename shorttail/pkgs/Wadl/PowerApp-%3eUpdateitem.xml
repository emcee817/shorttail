    </jsonTypes>
  </grammars>
  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://australia-001.azure-apim.net/apim/logicflows">
    <resource path="/{connectionId}/triggers/manual/run">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="Run">
        <doc title="Runs a logic flow trigger." />
        <request>
          <param style="query" name="api-version" type="xs:string" required="true" fixed="2015-02-01-preview">
            <doc title="The api version" />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="Updateitem_Id" path="/Updateitem_Id" type="xs:long" required="true" />
            <param style="plain" name="Updateitem_Title" path="/Updateitem_Title" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ1" path="/Updateitem_LodgementQ1" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ1Comment1" path="/Updateitem_LodgementQ1Comment1" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ1Comment2" path="/Updateitem_LodgementQ1Comment2" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ2" path="/Updateitem_LodgementQ2" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ2Comment1" path="/Updateitem_LodgementQ2Comment1" type="xs:string" required="true" />
            <param style="plain" name="Updateitem_LodgementQ2Comment2" path="/Updateitem_LodgementQ2Comment2" type="xs:string" required="true" />
          </representation>
        </request>
      </method>
    </resource>
  </resources>
  <authenticationProviders xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
    <connectionProvider id="PowerAppAuth" siena:connectionProviderId="logicflows" />
  </authenticationProviders>
  <template xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
    <variable name="connectionId">
      <modifyParams style="template" name="connectionId" attribute="fixed" />
    </variable>
  </template>
</application>