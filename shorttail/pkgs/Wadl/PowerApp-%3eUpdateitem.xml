<application xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:service="https://australia-001.azure-apim.net/apim/logicflows" xmlns:siena="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11" siena:serviceId="PowerApp-&gt;Updateitem" xmlns="http://wadl.dev.java.net/2009/02">
  <doc title="PowerApp -&gt; Update item" />
  <grammars>
    <jsonTypes targetNamespace="https://australia-001.azure-apim.net/apim/logicflows" xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
      <integer name="ManualTriggerInput_Updateitem_Id_def" />
      <string name="ManualTriggerInput_Updateitem_Title_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ1_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ1Comment1_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ1Comment2_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ2_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ2Comment1_def" />
      <string name="ManualTriggerInput_Updateitem_LodgementQ2Comment2_def" />
