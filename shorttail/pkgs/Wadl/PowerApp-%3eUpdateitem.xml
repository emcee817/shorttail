        <property name="Updateitem_Id" type="integer" />
        <property name="Updateitem_Title" type="string" />
        <property name="Updateitem_LodgementQ1" type="string" />
        <property name="Updateitem_LodgementQ1Comment1" type="string" />
        <property name="Updateitem_LodgementQ1Comment2" type="string" />
        <property name="Updateitem_LodgementQ2" type="string" />
        <property name="Updateitem_LodgementQ2Comment1" type="string" />
        <property name="Updateitem_LodgementQ2Comment2" type="string" />
